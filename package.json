{
  "name": "suzumiyaaoba-github-io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run clean && npm run pagefind:dev && run-p next:dev tsm",
    "next:dev": "next dev",
    "build": "run-s clean copy:assets build:next build:pagefind fix:pagefind",
    "build:next": "NODE_ENV=production next build",
    "build:pagefind": "pagefind --site out --output-path out/pagefind",
    "fix:pagefind": "node scripts/fix-pagefind-module.js",
    "pagefind:dev": "node scripts/pagefind-dev.js",
    "clean": "rimraf .next out",
    "copy:assets": "run-p copy:blog:images copy:blog:css copy:notes:images",
    "copy:blog:images": "cpy 'src/contents/blog/**/*.{png,jpg,jpeg,svg}' 'public/assets/blog'",
    "copy:notes:images": "cpy 'src/contents/notes/**/*.{png,jpg,jpeg,svg}' 'public/assets/notes'",
    "copy:blog:css": "cpy 'src/contents/blog/**/*.css' 'public/assets/blog'",
    "start": "next start",
    "tsm": "typed-scss-modules src --watch --implementation sass --nameFormat none --exportType default --aliasPrefixes.@ src/",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "updates": "npx npm-check-updates",
    "test": "vitest run",
    "test:watch": "vitest"
  },
